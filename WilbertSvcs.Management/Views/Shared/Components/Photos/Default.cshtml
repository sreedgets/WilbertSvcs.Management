@model IEnumerable< WilbertVaultCompany.api.Models.FuneralHomeContact>
<table>
    <tbody>

        <tr>
            <td colspan="4"><label class="sectionLabel"></label><h3>Contact Information</h3></td>
        </tr>
        <tr align="left" style="color:White;background-color:#00446a;font-weight:bold;">
            <th scope="col"><a href="#" style="color:White;">Name</a></th>
            <th scope="col"><a href="#" style="color:White;">Title</a></th>
            <th scope="col"><a style="color:White;">Phone</a></th>
            <th scope="col"><a style="color:White;">&nbsp;</a></th>
        </tr>
        <tr style="color:#333333;background-color:#ECECEC;">
            <td style="width:18%;">Jordan Borcherding</td>
            <td style="width:10%;"><span>Funeral Director</span></td>
            <td style="width:10%;"><span>(319) 824-3319</span></td>
            <td style="width:10%;"><a href="#">Disable Login</a></td>
        </tr>
        @foreach (var contact in Model)
        {

            <tr style="color:#333333;background-color:#ECECEC;">
                <td style="width:18%;">
                    <label asp-for="@contact.FirstName" class="control-label"></label>
                    <input asp-for="@contact.FirstName" class="form-control" />
                    <span asp-validation-for="@contact.FirstName" class="text-danger"></span>
                </td>
                <td style="width:10%;">
                    <span>
                        <label asp-for="@contact.ContactRole" class="control-label"></label>
                        <input asp-for="@contact.ContactRole" class="form-control" />
                        <span asp-validation-for="@contact.ContactRole" class="text-danger"></span>
                    </span>
                </td>
                <td>

                    <label asp-for="@contact.Phone1"></label>
                </td>
                <td>
                    <input asp-for="@contact.Phone1" class="form-control" placeholder="xxx-xxx-xxxx" onkeydown="phoneOnKeyDown(this);" onblur="phoneOnBlur(this);" />
                    <span asp-validation-for="@contact.Phone1" class="text-danger"></span>

                </td>
                <td style="width:10%;"><a href="#">Disable Login</a></td>
            </tr>
        }
        <tr>
            <td>
                <a asp-action="Create" asp-controller="funeralhomecontacts" class="btn btn-secondary">Add Contacts</a>
            </td></tr>
    </tbody>
</table>